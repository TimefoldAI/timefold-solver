<?xml version="1.0" encoding="UTF-8"?>
<solver xmlns="https://www.optaplanner.org/xsd/solver" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://www.optaplanner.org/xsd/solver https://www.optaplanner.org/xsd/solver/solver.xsd">
  <domainAccessType>GIZMO</domainAccessType>
  <scoreDirectorFactory>
      <constraintProviderClass>org.optaplanner.spring.boot.autoconfigure.gizmo.constraints.TestdataGizmoConstraintProvider</constraintProviderClass>
  </scoreDirectorFactory>
  <termination>
    <minutesSpentLimit>3</minutesSpentLimit>
  </termination>
</solver>
